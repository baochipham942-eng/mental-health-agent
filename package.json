{
  "name": "mental-health-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "smoke": "ts-node --project tsconfig.scripts.json scripts/run-smoke.ts",
    "verify:config": "ts-node --project tsconfig.scripts.json scripts/verify-smoke-config.ts",
    "test:strip": "ts-node --project tsconfig.scripts.json scripts/conclusion-strip-smoke.ts",
    "test:unit": "ts-node --project tsconfig.scripts.json lib/ai/assessment/sanitize.test.ts",
    "ci:check": "cross-env SKILL_MODE=steps_and_cards npm run verify:config && cross-env SKILL_MODE=steps_and_cards npm run typecheck && cross-env SKILL_MODE=steps_and_cards npm run test:strip && cross-env SKILL_MODE=steps_and_cards npm run test:unit && cross-env SKILL_MODE=steps_and_cards npm run smoke",
    "deploy:build": "npx prisma generate && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.54",
    "@arco-design/web-react": "^2.66.8",
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "5.22.0",
    "ai": "^3.4.0",
    "baidu-aip-sdk": "^4.16.16",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.26",
    "groq-sdk": "^0.37.0",
    "langfuse": "^3.38.6",
    "next": "^14.2.0",
    "next-auth": "^5.0.0-beta.30",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.33",
    "prisma": "5.22.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
